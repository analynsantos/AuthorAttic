<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.authorattic">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/book_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/book_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.android.authorattic.MainActivity"></activity>
        <activity
            android:name="com.example.android.authorattic.InfoActivity"
            android:parentActivityName="com.example.android.authorattic.MainActivity">
        </activity>
    </application>

</manifest>